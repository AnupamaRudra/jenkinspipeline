pipeline{

   
     agent any

      stages{
 		      stage('One')
		          	{
                      steps{
                       echo "Hi"
	                   }
                              }
			
 		        stage('Two')
			        {
                            steps{
                               input( "Are you fine?")
				 }

			        }
 		          stage('Three')
			          {
                            steps{
                               echo "Hello"
				  }

			           }
 		
            }
}
